<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="@dimen/joy_base_width"
            android:layout_height="320dp"
            android:layout_gravity="center"
            android:background="@drawable/joy_protocol_bg"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp">

                <TextView
                    android:layout_marginTop="30dp"
                    android:id="@+id/l_protocol_title"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:padding="5dp"
                    android:textColor="@color/joygame_black"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <FrameLayout
                    android:id="@+id/l_protocol_back"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_marginLeft="30dp"/>
                <FrameLayout
                    android:layout_alignParentRight="true"
                    android:id="@+id/l_protocol_back2"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_marginRight="30dp"/>

            </RelativeLayout>


            <WebView
                android:id="@+id/k_frg_protocol_web_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/joy_inde_l_r_padding"
                android:layout_marginRight="@dimen/joy_inde_l_r_padding"
                android:layout_marginBottom="45dp"
                android:orientation="vertical">

            </WebView>

        </LinearLayout>
    </LinearLayout>

</FrameLayout>
